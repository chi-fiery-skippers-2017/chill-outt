<h2><%= @article.title %></h2>

<label>Versions</label>

<div class="version-container">
  <p class="version-content">
    <%= @article.versions.each do |version| %>
      <p class="version-content">
        <%= version.content %>
        <small>-Written by: <%= version.author.username %> on <%= version.</small>
      </p>
    <% end %>
</div>
